{
  "build": {
    "command": "npm run build",
    "output_directory": "out",
    "root_directory": "."
  },
  "routes": [
    { "pattern": "/game/*", "script": null, "serve": "/game/index.html" },
    { "pattern": "/admin/*", "script": null, "serve": "/admin/index.html" },
    { "pattern": "/404.html", "script": null, "serve": "/404.html" }
  ],
  "environment_variables": {
    "NODE_VERSION": "18.x"
  }
} 